{
  "name": "@react-symbols/icons",
  "version": "1.2.9",
  "description": "Symbols icons by Miguel Solorio for React",
  "author": "@pheralb_",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "react",
    "icons",
    "svg",
    "icons",
    "lightweight",
    "symbols",
    "devicons",
    "ui",
    "ux",
    "interface"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pheralb/react-symbols.git"
  },
  "contributors": [
    {
      "name": "pheralb",
      "github": "https://github.com/pheralb"
    },
    {
      "name": "miguelsolorio",
      "github": "https://github.com/miguelsolorio"
    }
  ],
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "typings": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "types": "./build/index.d.ts"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "dev": "tsc && swc ./src -d ./build",
    "build:swc-tsc": "tsc && swc ./src -d ./build && tsc --emitDeclarationOnly --declaration --outDir build/types",
    "build": "tsc && swc ./src -d ./build",
    "prepublishOnly": "npm run build",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@swc/cli": "0.7.3",
    "@swc/core": "1.11.24",
    "@types/react": "19.1.2",
    "@types/react-dom": "19.1.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tsup": "8.4.0",
    "typescript": "5.8.3"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  }
}
