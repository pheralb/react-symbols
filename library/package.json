{
  "name": "@react-symbols/icons",
  "version": "1.3.0",
  "description": "Symbols icons by Miguel Solorio for React",
  "author": "@pheralb_",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "react",
    "icons",
    "svg",
    "icons",
    "lightweight",
    "symbols",
    "devicons",
    "ui",
    "ux",
    "interface"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pheralb/react-symbols.git"
  },
  "contributors": [
    {
      "name": "pheralb",
      "github": "https://github.com/pheralb"
    },
    {
      "name": "miguelsolorio",
      "github": "https://github.com/miguelsolorio"
    }
  ],
  "main": "./build/index.mjs",
  "module": "./build/index.mjs",
  "types": "./build/index.d.mts",
  "typings": "./build/index.d.mts",
  "exports": {
    ".": {
      "import": "./build/index.mjs",
      "types": "./build/index.d.mts"
    },
    "./files": {
      "import": "./build/files/index.mjs",
      "types": "./build/files/index.d.mts"
    },
    "./folders": {
      "import": "./build/folders/index.mjs",
      "types": "./build/folders/index.d.mts"
    },
    "./utils": {
      "import": "./build/utils/index.mjs",
      "types": "./build/utils/index.d.mts"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "dev": "tsdown --watch",
    "build": "eslint ./src && tsdown",
    "prepublishOnly": "pnpm run build",
    "lint": "eslint ./src",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@eslint/compat": "2.0.1",
    "@eslint/js": "9.39.2",
    "@tsconfig/strictest": "2.0.8",
    "@types/node": "22.19.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "eslint": "9.39.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "17.0.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "tsdown": "0.18.4",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0"
  },
  "peerDependencies": {
    "react": ">= 16.8",
    "react-dom": ">= 16.8"
  }
}
