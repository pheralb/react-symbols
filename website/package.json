{
  "name": "@react-symbols/website",
  "version": "4.2.2",
  "description": "React Symbols Icons Website - Built with React Router 7.",
  "author": "@pheralb_",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm build:registry && react-router build",
    "build:registry": "tsx ./app/generators/generate-shadcn-registry.ts",
    "build:shadcn": "shadcn build",
    "build:extensions": "tsx ./app/generators/generate-icons-extensions.ts",
    "dev": "react-router dev",
    "lint": "react-router typegen && eslint ./app",
    "lint:fix": "react-router typegen && eslint ./app --fix",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc --noEmit"
  },
  "dependencies": {
    "@pheralb/toast": "1.0.0",
    "@radix-ui/react-collapsible": "1.1.12",
    "@radix-ui/react-dropdown-menu": "2.1.16",
    "@radix-ui/react-popover": "1.1.15",
    "@radix-ui/react-slider": "1.3.6",
    "@radix-ui/react-slot": "1.2.4",
    "@radix-ui/react-tooltip": "1.2.8",
    "@react-router/fs-routes": "7.12.0",
    "@react-router/node": "7.12.0",
    "@react-router/serve": "7.12.0",
    "@react-symbols/icons": "workspace:*",
    "@vercel/react-router": "1.2.4",
    "axios": "1.13.2",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "fast-npm-meta": "0.4.7",
    "isbot": "5.1.32",
    "lucide-react": "0.562.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-router": "7.12.0",
    "remix-themes": "2.0.4",
    "shadcn": "3.6.3",
    "sugar-high": "0.9.5",
    "tailwind-merge": "3.4.0"
  },
  "devDependencies": {
    "@eslint/compat": "2.0.1",
    "@eslint/js": "9.39.2",
    "@react-router/dev": "7.12.0",
    "@tailwindcss/vite": "4.1.18",
    "@types/node": "22.19.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "autoprefixer": "10.4.23",
    "eslint": "9.39.2",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react-hooks": "7.0.1",
    "eslint-plugin-react-refresh": "0.4.26",
    "globals": "17.0.0",
    "tailwindcss": "4.1.18",
    "tsx": "4.21.0",
    "tw-animate-css": "1.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.52.0",
    "vite": "7.3.1",
    "vite-plugin-devtools-json": "1.0.0"
  }
}
