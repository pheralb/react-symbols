    "@radix-ui/react-tooltip": "1.2.4",
    "@react-router/fs-routes": "7.5.3",
    "@react-router/node": "7.5.3",
    "@react-router/serve": "7.5.3",
    "@react-symbols/icons": "workspace:*",
    "@vercel/react-router": "1.1.0",
    "axios": "1.9.0",
    "change-case": "5.4.4",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
